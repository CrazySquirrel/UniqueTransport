{
  "extends": "tslint:latest",
  "rules": {
    "no-bitwise": false,
    "max-line-length": [
      true,
      320
    ],
    "no-var-requires": false,
    "variable-name": false,
    "no-reference": false,
    "trailing-comma": false,
    "comment-format": false,
    "object-literal-key-quotes": false,
    "no-string-literal": false
  }
}