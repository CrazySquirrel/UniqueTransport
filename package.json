{
  "name": "UniqueTransport",
  "version": "1.0.38",
  "description": "This plugin is designed to deliver a message from Alice to Bob at any cost.",
  "keywords": [
    "TypeScript",
    "Unique transport"
  ],
  "author": "Sergey Yastrebov <info@crazysquirrel.ru>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:CrazySquirrel/UniqueTransport.git"
  },
  "scripts": {
    "development:server:watch": "NODE_ENV=development webpack --config=webpack.config.server.js --watch",
    "development:client:watch": "NODE_ENV=development webpack --config=webpack.config.client.js --watch",
    "development:server": "NODE_ENV=development webpack --bail --config=webpack.config.server.js",
    "development:client": "NODE_ENV=development webpack --bail --config=webpack.config.client.js",
    "production:server:watch": "NODE_ENV=production webpack --config=webpack.config.server.js --watch",
    "production:client:watch": "NODE_ENV=production webpack --config=webpack.config.client.js --watch",
    "production:server": "NODE_ENV=production webpack --bail --config=webpack.config.server.js",
    "production:client": "NODE_ENV=production webpack --bail --config=webpack.config.client.js",
    "karma": "npm run karma:client && npm run karma:server",
    "karma:client": "rm -rf ./doc/report/client/ && rm -rf ./doc/coverage/client/ && NODE_ENV=testing karma start karma.client.conf.js",
    "karma:server": "rm -rf ./doc/report/server/ && rm -rf ./doc/coverage/server/ && NODE_ENV=testing karma start karma.server.conf.js",
    "stats": "npm run stats:client && npm run stats:server",
    "stats:client": "NODE_ENV=development webpack --bail --config=webpack.config.client.js --profile --json > ./doc/webpack.config.karma.client.js",
    "stats:server": "NODE_ENV=development webpack --bail --config=webpack.config.server.js --profile --json > ./doc/webpack.config.karma.server.js",
    "docs": "npm run docs:client && npm run docs:server",
    "docs:client": "rm -rf ./doc/js/client/ && typedoc --mode file --target ES6 --module 'commonjs' --out ./doc/js/client/ ./src/ts/client.ts",
    "docs:server": "rm -rf ./doc/js/server/ && typedoc --mode file --target ES6 --module 'commonjs' --out ./doc/js/server/ ./src/ts/server.ts"
  },
  "pre-commit": [],
  "eslintConfig": {
    "env": {
      "es6": true
    }
  },
  "dependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-runtime": "^6.20.0",
    "browser-sync": "^2.15.0",
    "browser-sync-webpack-plugin": "^1.1.2",
    "btoa": "^1.1.2",
    "clean-webpack-plugin": "^0.1.13",
    "compression": "^1.6.2",
    "compression-webpack-plugin": "^0.3.1",
    "crypto-js": "^3.1.8",
    "eslint": "^3.4.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-entities": "^1.2.0",
    "html-loader": "^0.4.4",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
    "jasmine-expect": "^3.0.1",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-browserstack-launcher": "^1.1.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-matchers": "^3.0.1",
    "karma-opera-launcher": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-source-map-support": "^1.2.0",
    "karma-webpack": "^1.8.0",
    "nodemon": "^1.11.0",
    "pngjs": "^3.0.1",
    "promise-polyfill": "^6.0.2",
    "raw-loader": "^0.5.1",
    "sass-lint": "^1.10.2",
    "string-replace-webpack-plugin": "https://github.com/CrazySquirrel/string-replace-webpack-plugin.git",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "typedoc": "^0.5.1",
    "typescript": "^2.0.9",
    "webpack": "^1.13.3",
    "webpack-closure-compiler": "^2.1.2",
    "webpack-notifier": "^1.4.1"
  }
}
